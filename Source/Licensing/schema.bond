namespace Fossa.Licensing


struct SystemLicenseEntitlements
{
    0: blob SystemId;
    1: string EnvironmentName;
    2: int32 MaximumCompanyCount;
    3: vector<string> CountryCodes;
}

struct CompanyLicenseEntitlements
{
    0: blob SystemId;
    1: int64 CompanyId;
    2: int32 MaximumBranchCount;
    3: int32 MaximumEmployeeCount;
    4: int32 MaximumDepartmentCount;
}
